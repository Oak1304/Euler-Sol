import math
number = 0
numbers = []
for x in range(2,20):
    number = x
    prime = True
    power = False
    for i in range (2,20):
        if (x%i==0 and x!=i):
            prime = False
            for n in range(2,5):
                if x == i**n:
                    power = True
                    occurences = numbers.count(i)
                    numbers.extend([i]*(n-occurences))
        if power = False:
            
                        
    if prime == True:
        numbers.append(x)
            
    
            
print(numbers)
print(math.prod(numbers))
            
